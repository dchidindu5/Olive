class base_class {
  init(info) {
    print info;
  }
  
  print_info() {
  	print "Base class method.";
  }
}

class derived_class : base_class {
  init(info) {
    print info;
  }
  
  print_info() {
    base.print_info();
    print "Derived class method.";
  }
}

var a = base_class("I'm the based class.");
a.print_info();

var b = derived_class("I'm the derived class.");
b.print_info();
